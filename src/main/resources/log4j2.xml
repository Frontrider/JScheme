<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="info">

    <Appenders>
        <File name="JSchemeFileAppender" fileName="logs/jscheme_log">
            <PatternLayout>
                <Pattern>%d %p [%t]: %m%n</Pattern>
            </PatternLayout>
        </File>
    </Appenders>

    <Loggers>
        <Root level="debug">
            <AppenderRef ref="JSchemeFileAppender"/>
        </Root>
    </Loggers>

</Configuration>
